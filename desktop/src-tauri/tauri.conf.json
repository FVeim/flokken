{
  "$schema": "https://schema.tauri.app/config/1",
  "productName": "desktop",
  "version": "0.1.0",
  "identifier": "flokken",
  "build": {
    "beforeDevCommand": "cd ../frontend && npm run dev",
    "beforeBuildCommand": "cd ../frontend && npm run build",
    "frontendDist": "../frontend/dist",
    "devUrl": "http://localhost:4322"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "desktop",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": []
  }
}
